<!doctype html>
<html>
  <head>
    <!-- init -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Cansur</title>

    <!-- <link rel="stylesheet" href="tmp/style.css"> -->

    <!-- Bootstap link -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

  </head>
  <body class="bg-dark text-light">

    <!-- NavBar -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary" data-bs-theme="dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="http://localhost:8080/">Cansur</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="http://localhost:8080/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="http://localhost:8080/Users">Users</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Temp</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <form id="boardForm" name="boardForm" method="post" action="/board">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>번호</th>
                        <th>제목</th>
                        <th>작성자</th>
                        <th>날짜</th>
                        <th>조회수</th>
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach((el,index) => {%>
                        <tr>
                            <!-- <th scope="row"> -->
                            <td><%= el.seq %></td>
                            <td><a href="http://localhost:8080/board/view/<%= el.seq %>"><%= el.title %></a></td>
                            <td><%= el.user_id %></td>
                            <td><%= el.created_time %></td>
                            <td><%= el.likes %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
            
            <div>            
                <a href='#' onClick='fn_write()' class="btn btn-success">글쓰기</a>            
            </div>
        </form>
    </div>
       
       

    <!-- Bootstap Script -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

  </body>
</html>